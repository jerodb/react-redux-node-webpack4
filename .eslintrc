{
  "extends": "airbnb",
  "globals": {
    "fetch": true,
    "document": true,
    "window": true
  },
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "no-use-before-define": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": 0,
    "semi": [2, "never"],
    "react/prop-types": "off",
    "comma-dangle": 0,
    "import/prefer-default-export": "off",
    "max-len": "off"
  }
}
